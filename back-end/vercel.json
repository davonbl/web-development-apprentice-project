{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        { 
            "src": "vite.config.js", 
            "use": "@vercel/static-build", 
            "config": { 
                "distDir": "dist" 
                } 
        }
    ],
    
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}